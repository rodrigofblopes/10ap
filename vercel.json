{
  "version": 2,
  "rewrites": [
    {
      "source": "/((?!assets/|.*\\.csv|.*\\.glb|.*\\.wasm|.*\\.js|.*\\.css|.*\\.html|.*\\.jpg|.*\\.png|.*\\.ico).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.csv)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/csv; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*\\.glb)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "model/gltf-binary"
        }
      ]
    }
  ]
}